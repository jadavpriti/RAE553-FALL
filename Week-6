<h5>using own name</h5>
<ul style="list-style-type:disc:">
<li>from flask import Flask
from flask_restful import Resource,Api

app = Flask(_name_)
api = Api(app)

class Student(Resource) :
 	def grt(self , name) :
		return { ‘ student ‘ : name}

api.add_resource(Student, ‘/student/<string:name>’)  #http://127.0.0.1:5000/student/priti

app.run(port=5000)<li>
